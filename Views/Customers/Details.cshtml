@model Vidly_MVCProject.Models.Customer
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li><b>Membership Type: @Model.MembershipType.Name</b></li>
    @if(@Model.Birthdate.HasValue){
    <li><b>Birthdate: @Model.Birthdate.Value.ToShortDateString()</b></li>
    }
</ul>